# tmux key-bindings

bind-key -n S-F12 source $HOME/.tmux.keys.enable \; display-message "tmux F-keys enabled. Press S-F12 to disable."
unbind-key -n F2
unbind-key -n S-F2
unbind-key -n C-F2
unbind-key -n C-S-F2

unbind-key -n F3
unbind-key -n S-F3

unbind-key -n F4
unbind-key -n S-F4

unbind-key -n F7

unbind-key -n F8
unbind-key -n C-F8
